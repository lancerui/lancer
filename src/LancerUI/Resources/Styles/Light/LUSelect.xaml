<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--边框色-->
    <SolidColorBrush x:Key="LUSelectBorderBrush" Color="#eaeaea" />
    <!--副边框色-->
    <SolidColorBrush x:Key="LUSelectSubBorderBrush" Color="#cccccc"/>
    <!--背景色-->
    <SolidColorBrush x:Key="LUSelectBgBrush" Color="#FEFFFF" />
    <!--前景色-->
    <SolidColorBrush x:Key="LUSelectForegroundBrush" Color="#242424" />
    <!--图标色-->
    <SolidColorBrush x:Key="LUSelectIconBrush" Color="#7a7a7a" />
    <!--鼠标经过背景色-->
    <SolidColorBrush x:Key="LUSelectHoverBgBrush" Color="#fcfbfc" />
    <!--弹出层背景色-->
    <SolidColorBrush x:Key="LUSelectPopupBgBrush" Color="#fbfbfb" />
    <!--弹出层边框色-->
    <SolidColorBrush x:Key="LUSelectPopupBorderBrush" Color="#dfdfe0" />
    <!--禁用背景色-->
    <SolidColorBrush x:Key="LUSelectDisabledBgBrush" Color="#F5F5F5" />
    <!--禁用前景色-->
    <SolidColorBrush x:Key="LUSelectDisabledForegroundBrush" Color="#9D9D9D" />
    <!--禁用边框色-->
    <SolidColorBrush x:Key="LUSelectDisabledBorderBrush" Color="#E3E3E3" />
</ResourceDictionary>