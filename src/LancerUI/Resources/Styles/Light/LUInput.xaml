<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--主边框色-->
    <SolidColorBrush x:Key="LUInputMainBorderBrush" Color="#e5e5e5"/>
    <!--副边框色-->
    <SolidColorBrush x:Key="LUInputSubBorderBrush" Color="#868686"/>
    <!--前景色-->
    <SolidColorBrush x:Key="LUInputForegroundBrush" Color="#1b1b1b" />
    <!--输入框默认背景-->
    <SolidColorBrush x:Key="LUInputBgBrush" Color="#fbfbfb" />
    <!--鼠标经过背景色-->
    <SolidColorBrush x:Key="LUInputHoverBgBrush" Color="#f6f6f6" />
    <!--获得焦点背景色-->
    <SolidColorBrush x:Key="LUInputFocusedBgBrush" Color="#fff" />
    <!--禁用背景色-->
    <SolidColorBrush x:Key="LUInputDisabledBgBrush" Color="#F5F5F5" />
    <!--禁用前景色-->
    <SolidColorBrush x:Key="LUInputDisabledForegroundBrush" Color="#9D9D9D" />
    <!--禁用边框色-->
    <SolidColorBrush x:Key="LUInputDisabledBorderBrush" Color="#E3E3E3" />
</ResourceDictionary>