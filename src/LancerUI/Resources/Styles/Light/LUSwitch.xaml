<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--【Default】-->
    <!--开关状态Switch颜色-->
    <SolidColorBrush x:Key="LUSwitchOffBrush" Color="#f5f5f5" />
    <!--开关状态Thumb颜色-->
    <SolidColorBrush x:Key="LUSwitchOnThumbBrush" Color="#fff" />
    <SolidColorBrush x:Key="LUSwitchOffThumbBrush" Color="#5d5d5d" />
    <!--鼠标经过颜色-->
    <SolidColorBrush x:Key="LUSwitchOnHoverBrush" Color="{DynamicResource ThemeColor}" Opacity=".7"/>
    <SolidColorBrush x:Key="LUSwitchOffHoverBrush" Color="#eeefef" />
</ResourceDictionary>